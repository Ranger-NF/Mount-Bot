{
    "compilerOptions": {
      "target": "ES2020",
      "module": "NodeNext",
      "rootDir": "./src",
      "outDir": "./dist",
      "removeComments": true,
      "moduleResolution": "NodeNext",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
    //   "strict": true,
      "strictNullChecks": true,
      "skipLibCheck": true
    }
  }